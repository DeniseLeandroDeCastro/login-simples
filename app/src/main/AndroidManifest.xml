<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.livroandroid.helloandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_hello_world"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_hello_world"
        android:supportsRtl="true"
        android:theme="@style/Theme.HelloAndroid">
        <activity android:name=".BemVindoActivity"></activity>
        <activity android:name=".DebugActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>